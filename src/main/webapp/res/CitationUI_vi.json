  "apa-book": [
    {
      "Box": "1",
      "Display": "Tên tác giả",
      "Hint": "Format: First Middle Last",
      "Important": "false",
      "Pattern": ".*"
    },
    {
      "Box": "1",
      "Display": "Editor's Name",
      "Hint": "Format: First Middle Last",
      "Important": "false",
      "Pattern": ".*"
    },
    {
      "Box": "1",
      "Display": "Organizations, corporations, etc",
      "Hint": "e.g. NXB Trẻ",
      "Important": "false",
      "Pattern": ".*"
    },
    {
      "Box": "2",
      "Display": "Title of book: Subtitle of book",
      "Hint": "e.g. Just business: Business ethics in action, Verge",
      "Important": "true",
      "Pattern": ".*"
    },
    {
      "Box": "2",
      "Display": "Year of Publication",
      "Hint": "e.g. 2000",
      "Important": "true",
      "Pattern": "[0-9][0-9][0-9][0-9]"
    },
    {
      "Box": "2",
      "Display": "Edition",
      "Hint": "e.g 12",
      "Important": "false",
      "Pattern": "[0-9]+"
    },
    {
      "Box": "2",
      "Display": "Publisher (*if is not an eBook)",
      "Hint": "e.g. Oxford University Press",
      "Important": "true",
      "Pattern": ".*"
    },
    {
      "Box": "2",
      "Display": "URL (*if is an eBook)",
      "Hint": "e.g. http://www.example.com/sub/address",
      "Important": "true",
      "Pattern": ".*"
    }
  ],
